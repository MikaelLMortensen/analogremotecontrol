geSpeed = 0\n    let oldVoltageHeading = 0\n    let oldSpeed = 0\n    let oldHeading = 0\n\n    voltageSpeed = pins.analogReadPin(AnalogPin.P2)\n    // Has speed input changed since last run\n    if (voltageSpeed != oldVoltageSpeed) {\n        oldVoltageSpeed = voltageSpeed\n        speed = 0\n        // calculate speed (0 - 100)\n        if (voltageSpeed > 5) {\n            // convert voltage to percent 1023 => 100\n            speed = Math.floor(voltageSpeed / 10.2)\n        }\n        // if speed has changed we send it\n        if (speed != oldSpeed) {\n            oldSpeed = speed\n            sendSpeed()\n        }\n    }\n    voltageHeading = pins.analogReadPin(AnalogPin.P1)\n    // Has heading input changed since last run\n    if (voltageHeading != oldVoltageHeading) {\n        oldVoltageHeading = voltageHeading\n        heading = Math.floor(voltageHeading / (5.1)) - 100 // Range: -100 => 100\n        sendHeading()\n    }\n    basic.pause(100)\n})"],[1,"\n"]],"start1":0,"start2":0,"length1":2064,"length2":1}]},{"type":"edited","filename":"pxt.json","patch":[{"diffs":[[0,"   \""],[-1,"preferredEditor\": \"tsprj\""],[1,"additionalFilePaths\": []"],[0,"\n}\n"]],"start1":243,"start2":243,"length1":32,"length2":31}]}]},{"timestamp":1735470625792,"editorVersion":"7.0.51","changes":[{"type":"edited","filename":"main.blocks","patch":[{"diffs":[[0,"art\""],[-1," id=\"4/CW~.ebg|VfNr~@RP8;\""],[0," x=\""]],"start1":100,"start2":100,"length1":34,"length2":8},{"diffs":[[0,"ver\""],[-1," id=\"qmq2TnjNKKJ-|]#r,)7V\""],[0," x=\""]],"start1":151,"start2":151,"length1":34,"length2":8}]}]},{"timestamp":1735472370764,"editorVersion":"7.0.51","changes":[{"type":"edited","filename":"pxt.json","patch":[{"diffs":[[0," ],\n"],[-1,"    \"testFiles\": [\n        \"test.ts\"\n    ],\n"],[0,"    "]],"start1":238,"start2":238,"length1":52,"length2":8}]},{"type":"added","filename":"test.ts","value":"// test ender her, dette bliver ikke kompileret, når denne pakke bruges som en udvidelse.\n